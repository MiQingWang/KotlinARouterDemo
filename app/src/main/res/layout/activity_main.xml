<?xml version="1.0" encoding="utf-8"?>
<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/qmui_config_color_background">

<com.qmuiteam.qmui.widget.QMUIViewPager
    android:id="@+id/app_view_pager"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="150mm"
    tools:ignore="InOrMmUsage" />

<com.qmuiteam.qmui.widget.tab.QMUITabSegment
    android:id="@+id/tab_segment"
    android:layout_width="match_parent"
    android:layout_height="150mm"
    android:layout_gravity="bottom"
    android:background="@color/qmui_config_color_white"
    app:qmui_tab_icon_position="top"
    app:qmui_tab_normal_text_size="12sp"
    app:qmui_tab_selected_text_size="13sp"
    app:qmui_topDividerColor="?attr/qmui_skin_support_color_separator"
    app:qmui_topDividerHeight="1mm"
    tools:ignore="InOrMmUsage">



</com.qmuiteam.qmui.widget.tab.QMUITabSegment>


</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>